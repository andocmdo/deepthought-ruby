

      <p id="notice"><%= notice %></p>

      <h1>Jobs</h1>
      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Worker</th>
              <th>Job valid</th>
              <th>Dispatched</th>
              <th>Completed</th>
              <th>Success</th>
              <th>Command</th>
              <th>Output</th>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody id="job-table">
              <%= render @jobs %>
          </tbody>
        </table>
      </div>
      <br>
      <div class="text-center">
        <%= link_to 'New Job', new_job_path, class: 'btn btn-info', role: 'button' %>
      </div>
