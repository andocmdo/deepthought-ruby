<p id="notice"><%= notice %></p>

<h1>Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Worker</th>
      <th>Job valid</th>
      <th>Dispatched</th>
      <th>Completed</th>
      <th>Success</th>
      <th>Command</th>
      <th>Output</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.worker_id %></td>
        <td><%= job.job_valid %></td>
        <td><%= job.dispatched %></td>
        <td><%= job.completed %></td>
        <td><%= job.success %></td>
        <td><%= job.command %></td>
        <td><%= job.output %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
